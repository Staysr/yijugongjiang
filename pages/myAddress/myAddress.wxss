/* pages/myAddress/myAddress.wxss */

page
{
  background: #ebebeb;
}
.myAddressList
{
  margin: 40rpx 0;
}
.myAddressInforMation
{
  background: #fff;
  padding: 20rpx 20rpx
}
.setDefaluteAddress
{
  height: 75rpx;
  line-height: 75rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #ebebeb;
}
.addressOperation
{
  font-size: 26rpx;
   display: flex;
  justify-content: flex-end;
  align-items: center;
}
.addressEdit ,.addressDelet
{
display: flex;
justify-content: flex-end;
align-items: center;

}
.addressDelet
{
  margin-left: 40rpx;
}
.addressEdit image , .addressDelet image
{
width: 40rpx;
height: 40rpx;
}
.myAddressName
{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
}

.myAddressContent
{
    height: 40rpx;
  line-height: 40rpx;  font-size: 26rpx;
}
.setDefaluteAddress
{
  padding: 0 20rpx
}
.setDefalutAddressBtn
{
  font-size: 26rpx
}


.setUp02 ,.setUp01
{

    width: 34rpx;
  height: 34rpx;
}
.setUp02
{
  display: none;
}
.selectAddress .setUp01
{
  display: none;
}
.selectAddress .setUp02
{
  display: block;
}
.setDefalutText
{
  color: #666;
  font-size: 26rpx;
  margin-left: 10rpx;
}
.setDefalutAddressBtn
{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.addAddressBox
{
  height: 80rpx;
}
.addAddressBtn
{
  width: 100%;
  height: 100rpx;
  background: #06881c;
  color: #fff;
  line-height: 100rpx;
  font-size: 30rpx;
  text-align: center;
  border-radius: 0;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999
}