/* pages/releaseinformation/releaseinformation.wxss */
.mask {
	width: 100%;
	height: 100%;
	position: fixed;
	background-color: #8d8d8d;
	z-index: 999;
	top: 0;
	left: 0;
	opacity: 0.5;
}
.msgContent{
	position:fixed;
  z-index:9999;
  top:19%;
  background-color:#FFFFFF;
  margin:0 10% 0 10%;
  width:80%;

}
.msgTitle{
	background-color:#eeeeee;
	display:flex;
	justify-content:space-between;
	padding:25rpx;

}
.msgText{
	color: #333333;
	font-weight: 600;
}
.msgClose{
	color: #cccccc;
}
.msgs{
	color: #333333;
	font-size:35rpx;
	padding:50rpx 25rpx;
	line-height:60rpx;

}
.imgtext{
  width: 100%;
  height: 100%;
  text-align:center;
  font-size:60rpx;
  padding-top:60rpx;

}